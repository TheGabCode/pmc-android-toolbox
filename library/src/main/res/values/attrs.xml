<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/tools" android:ignore="NamespaceTypo">
    <declare-styleable name="CustomButton">
        <attr name="subText" format="string" />
        <attr name="subTextSize" format="dimension" />
        <attr name="subTextColor" format="color" />
        <attr name="primaryColor" format="color" />
        <attr name="secondaryColor" format="color" />
        <attr name="highlighted" format="boolean" />
        <attr name="roundedCorners" format="boolean" />
        <attr name="cornerRadius" format="dimension" />
        <attr name="icon" format="reference" />
        <attr name="android:paddingLeft" />
        <attr name="android:paddingRight" />
        <attr name="android:paddingStart" />
        <attr name="android:paddingEnd" />
        <attr name="android:paddingTop" />
        <attr name="android:paddingBottom" />
        <attr name="android:padding"/>
    </declare-styleable>
</resources>